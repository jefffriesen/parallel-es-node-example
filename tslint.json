{
  "extends": "tslint:latest",
  "rules": {
    "semicolon": false,
    "no-console": false,
    "only-arrow-functions": false,
    "arrow-parens": false,
    "no-consecutive-blank-lines": false,
    "max-line-length": 80,
    "quotemark": [true, "single"],
    "trailing-comma": [false, { "multiline": "never", "singleline": "never" }],
    "ordered-imports": false,
    "no-string-literal": false
  }
}
